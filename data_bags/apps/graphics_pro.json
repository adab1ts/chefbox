{
  "id": "graphics_pro",
  "apps": [
    "darktable",
    "draftsight",
    "freecad",
    "gimp",
    "inkscape",
    "librecad",
    "mypaint",
    "qcad",
    "rawtherapee",
    "scribus"
  ],
  "profiles": {
    "darktable": {
      "description": "Virtual lighttable and darkroom for photographers",
      "website": "http://www.darktable.org/",
      "platforms": {
        "debian-7.5":   { "source": "distro" },
        "linuxmint-13": { "source": "ppa" },
        "ubuntu-12.04": { "source": "ppa" },
        "ubuntu-14.04": { "source": "ppa" }
      },
      "sources": {
        "distro": {
          "package": "darktable",
          "maintainer": "Debian PhotoTools Maintainers <pkg-phototools-devel@lists.alioth.debian.org>"
        },
        "ppa": {
          "package": "darktable",
          "maintainer": "Pascal de Bruijn <pmjdebruijn@pcode.nl>",
          "meta": {
            "repo_name": "pmjdebruijn-darktable-release",
            "uri": "ppa:pmjdebruijn/darktable-release"
          }
        }
      }
    },
    "draftsight": {
      "description": "With DraftSight, you can easily create professional CAD drawings",
      "website": "http://www.3ds.com/products-services/draftsight/overview/",
      "platforms": {},
      "sources": {
        "deb": {
          "package": "draftsight",
          "maintainer": "Dassault Systemes <CrashReports@DraftSight.com"
        }
      }
    },
    "freecad": {
      "description": "Extensible Open Source CAx program",
      "website": "http://www.freecadweb.org/",
      "platforms": {
        "linuxmint-13": { "source": "ppa" },
        "ubuntu-12.04": { "source": "ppa" },
        "ubuntu-14.04": { "source": "distro" }
      },
      "sources": {
        "distro": {
          "package": "freecad",
          "maintainer": "Ubuntu Developers <ubuntu-devel-discuss@lists.ubuntu.com>",
          "suggested": ["freecad-doc"]
        },
        "ppa": {
          "package": "freecad",
          "maintainer": "Normand Chamberland <gemnoc@gmail.ca>",
          "suggested": ["freecad-doc", "python-matplotlib"],
          "meta": {
            "repo_name": "freecad-maintainers-freecad-stable",
            "uri": "ppa:freecad-maintainers/freecad-stable"
          }
        }
      }
    },
    "gimp": {
      "description": "The GNU Image Manipulation Program",
      "website": "http://www.gimp.org/",
      "platforms": {
        "debian-7.5":   { "source": "distro-wheezy", "default": true },
        "linuxmint-13": { "source": "ppa", "default": true },
        "ubuntu-12.04": { "source": "ppa" },
        "ubuntu-14.04": { "source": "distro", "default": true }
      },
      "sources": {
        "distro": {
          "package": "gimp",
          "maintainer": "Ubuntu Developers <ubuntu-devel-discuss@lists.ubuntu.com>",
          "suggested": ["gimp-data-extras", "gimp-gmic", "gimp-plugin-registry", "icc-profiles", "icc-profiles-free"]
        },
        "distro-wheezy": {
          "package": "gimp",
          "maintainer": "Ari Pollak <ari@debian.org>",
          "suggested": ["gimp-data-extras", "gimp-gmic", "gimp-plugin-registry", "icc-profiles", "icc-profiles-free"]
        },
        "ppa": {
          "package": "gimp",
          "maintainer": "Otto Meier <>",
          "suggested": ["gimp-gmic", "gimp-plugin-registry", "icc-profiles", "icc-profiles-free"],
          "meta": {
            "repo_name": "otto-kesselgulasch-gimp",
            "uri": "ppa:otto-kesselgulasch/gimp"
          }
        }
      }
    },
    "inkscape": {
      "description": "vector-based drawing program",
      "website": "http://www.inkscape.org/en/",
      "platforms": {
        "debian-7.5":   { "source": "distro-wheezy" },
        "linuxmint-13": { "source": "distro" },
        "ubuntu-12.04": { "source": "distro" },
        "ubuntu-14.04": { "source": "distro" }
      },
      "sources": {
        "distro": {
          "package": "inkscape",
          "maintainer": "Ubuntu Developers <ubuntu-devel-discuss@lists.ubuntu.com>"
        },
        "distro-wheezy": {
          "package": "inkscape",
          "maintainer": "Wolfram Quester <wolfi@sigxcpu.org>"
        }
      }
    },
    "librecad": {
      "description": "Computer-aided design (CAD) system",
      "website": "http://librecad.org/cms/home.html",
      "platforms": {
        "debian-7.5":   { "source": "distro" },
        "linuxmint-13": { "source": "ppa" },
        "ubuntu-12.04": { "source": "ppa" },
        "ubuntu-14.04": { "source": "ppa" }
      },
      "sources": {
        "distro": {
          "package": "librecad",
          "maintainer": "Debian Science Maintainers <debian-science-maintainers@lists.alioth.debian.org>"
        },
        "ppa": {
          "package": "librecad",
          "maintainer": "Debian Science Maintainers <debian-science-maintainers@lists.alioth.debian.org>",
          "meta": {
            "repo_name": "librecad-dev-librecad-stable",
            "uri": "ppa:librecad-dev/librecad-stable"
          }
        }
      }
    },
    "mypaint": {
      "description": "paint program for use with graphics tablets",
      "website": "http://mypaint.intilinux.com/",
      "platforms": {
        "debian-7.5":   { "source": "distro-wheezy" },
        "linuxmint-13": { "source": "ppa" },
        "ubuntu-12.04": { "source": "ppa" },
        "ubuntu-14.04": { "source": "distro" }
      },
      "sources": {
        "distro": {
          "package": "mypaint",
          "maintainer": "Ubuntu Developers <ubuntu-devel-discuss@lists.ubuntu.com>",
          "suggested": ["mypaint-data-extras"]
        },
        "distro-wheezy": {
          "package": "mypaint",
          "maintainer": "Gürkan Sengün <gurkan@phys.ethz.ch>"
        },
        "ppa": {
          "package": "mypaint",
          "maintainer": "Python Applications Packaging Team <python-apps-team@lists.alioth.debian.org>",
          "suggested": ["mypaint-data-extras"],
          "meta": {
            "repo_name": "achadwick-mypaint-testing",
            "uri": "ppa:achadwick/mypaint-testing"
          }
        }
      }
    },
    "qcad": {
      "description": "The Open Source 2D CAD",
      "website": "http://www.qcad.org/en/",
      "platforms": {
        "linuxmint-13": { "source": "bin" },
        "ubuntu-12.04": { "source": "bin" },
        "ubuntu-14.04": { "source": "bin" }
      },
      "sources": {
        "bin": {
          "ztype": "tgz",
          "package": "qcad",
          "maintainer": "Andrew Mustun <https://github.com/qcad>",
          "meta": {
            "i686": {
              "file_name": "qcad-3.4.7-linux-x86_32.tar.gz",
              "uri": "https://ocserver/public.php?service=files&t=3c10396ab79c46b28485e763487912a7&download",
              "sha256": "89480ad94c3adba2fb0f88e1eba4ed07173ff39b2d233bbd864cb0b6f831bb8f",
              "zfolder": "qcad-3.4.7-linux-x86_32"
            },
            "x86_64": {
              "file_name": "qcad-3.4.7-linux-x86_64.tar.gz",
              "uri": "https://ocserver/public.php?service=files&t=dfc6f95804fc3685737a7aa87e8a2755&download",
              "sha256": "fe005f22b3c0951ab77fc655ddacd91aea7b5217c8e195304ecd8f1c3c79f51a",
              "zfolder": "qcad-3.4.7-linux-x86_64"
            }
          }
        }
      }
    },
    "rawtherapee": {
      "description": "RAW file processor",
      "website": "http://www.rawtherapee.com/",
      "platforms": {
        "debian-7.5":   { "source": "distro-wheezy" },
        "linuxmint-13": { "source": "ppa" },
        "ubuntu-12.04": { "source": "ppa" },
        "ubuntu-14.04": { "source": "distro" }
      },
      "sources": {
        "distro": {
          "package": "rawtherapee",
          "maintainer": "Ubuntu Developers <ubuntu-devel-discuss@lists.ubuntu.com>"
        },
        "distro-wheezy": {
          "package": "rawtherapee",
          "maintainer": "Philip Rinn <rinni@gmx.net>"
        },
        "ppa": {
          "package": "rawtherapee",
          "maintainer": "Dariusz Duma <dhor@toxic.net.pl>",
          "meta": {
            "repo_name": "dhor-myway",
            "uri": "ppa:dhor/myway"
          }
        }
      }
    },
    "scribus": {
      "description": "Open Source Desktop Page Layout - stable branch",
      "website": "http://www.scribus.net/canvas/Scribus",
      "platforms": {
        "debian-7.5":   { "source": "distro-wheezy" },
        "linuxmint-13": { "source": "distro" },
        "ubuntu-12.04": { "source": "distro" },
        "ubuntu-14.04": { "source": "distro" }
      },
      "sources": {
        "distro": {
          "package": "scribus",
          "maintainer": "Ubuntu Developers <ubuntu-devel-discuss@lists.ubuntu.com>",
          "suggested": ["scribus-doc", "scribus-template", "icc-profiles", "icc-profiles-free"]
        },
        "distro-wheezy": {
          "package": "scribus",
          "maintainer": "Oleksandr Moskalenko <malex@debian.org>",
          "suggested": ["scribus-doc", "scribus-template", "icc-profiles", "icc-profiles-free"]
        }
      }
    }
  }
}
