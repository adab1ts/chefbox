{
  "name": "devel-box",
  "description": "Development box profile",
  "json_class": "Chef::Role",
  "chef_type": "role",
  "env_run_lists": {},
  "run_list": [
    "devel",
    "browsers",
    "cloud",
    "groupware",
    "sysadmin"
  ],
  "default_attributes": {
    "box": {
      "apps": {
        "browsers": [
          "chrome",
          "firefox",
          "opera"
        ],
        "cloud": [
          "dropbox"
        ],
        "devel": [
          "ansible",
          "atom",
          "bower",
          "brackets",
          "git",
          "grunt",
          "gulp",
          "nodejs",
          "ruby",
          "shelr",
          "vagrant",
          "virtualbox",
          "wsk",
          "yeoman",
          "zsh"
        ],
        "groupware": [
          "hangouts",
          "mumble",
          "smuxi"
        ],
        "sysadmin": [
          "htop",
          "speedtest"
        ]
      }
    }
  },
  "override_attributes": {}
}
