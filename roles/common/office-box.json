{
  "name": "office-box",
  "description": "Office box profile",
  "json_class": "Chef::Role",
  "chef_type": "role",
  "env_run_lists": {},
  "run_list": [
    "browsers",
    "cloud",
    "groupware",
    "office",
    "utils"
  ],
  "default_attributes": {
    "box": {
      "apps": {
        "browsers": [
          "chrome"
        ],
        "cloud": [
          "drive",
          "dropbox"
        ],
        "groupware": [
          "hangouts",
          "skype"
        ],
        "office": [
          "libreoffice",
          "pdfshuffler",
          "springseed",
          "taskcoach"
        ],
        "utils": [
          "shutter"
        ]
      }
    }
  },
  "override_attributes": {}
}
