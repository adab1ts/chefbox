        ],
        "indicators": [
          "plug&play",
          "screensaver",
          "touchpad",
          "ubuntuone",
          "weather"
        ],
        "office": [
          "fontmanager",
          "libreoffice",
          "masterpdf",
          "nitro",
          "pdfshuffler",
          "taskcoach"
        ],
        "security": [
          "antivirus",
