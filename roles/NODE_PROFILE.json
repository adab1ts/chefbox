{
  "name": "ENV-mKnnn-NAME",
  "description": "Profile for ENV-mKnnn-NAME box",
  "json_class": "Chef::Role",
  "chef_type": "role",
  "env_run_lists": {},
  "run_list": [
    "base::run-init",@@RUN_LIST@@,"base::run-end"
  ],
  "default_attributes": {
    "chef_report": {
      "recipient": "YOU@EXAMPLE.COM"
    },
    "box": {
      "name": "ENV-mKnnn-NAME",
      "platform": {
        "os": "OS",
        "version": "VERSION",
        "arch": "ARCH",
        "virtual": false
      },
      "lang": "LANG",
      "folders": {
        "admin": "ADMIN_FOLDER",
        "apps": "APPS_FOLDER",
        "desktop": "DESKTOP_FOLDER",
        "support": "FIRSTSTEPS_FOLDER"
      },
      "users": {
        "USER": {
          "uid": 1000,
          "gid": 1000,
          "group": "GROUP",
          "home": "/home/USER",
          "shell": "/bin/bash",
          "password": "secret",
          "default": true,
          "guest": false,
          "sudo": true
        }
      },
      "dotfiles": {
        "users": ["USER"],
        "folder": ".dotfiles"
      },
      "jobs": {
        "users": ["USER"],
        "folder": "ADMIN_FOLDER/jobs",
        "profiles": {
          "USER": {
            "backup": {
              "tasks": [
                {
                  "name": "backup-DRIVE",
                  "drive": "DRIVE_FOLDER",
                  "folder": "BACKUP_FOLDER",
                  "targets": ["home"]
                }
              ]
            }
          }
        }
      },
      "devel": {
        "users": ["USER"],
        "folder": "DEVEL_FOLDER"
      }
    }
  },
  "override_attributes": {
    "box": {
      "apps": {
        "audio": [
          "lastfm",
          "nuvola",
          "rdio",
          "rhythmbox",
          "spotify",
          "tomahawk"
        ],
        "audio_pro": [
          "airtime",
          "ardour",
          "audacity",
          "guitarix",
          "hydrogen",
          "idjc",
          "linuxband",
          "mixxx",
          "muse",
          "musescore",
          "rosegarden",
          "transcribe",
          "traverso",
          "tuxguitar"
        ],
        "browsers": [
          "chrome",
          "chromium",
          "firefox",
          "opera"
        ],
        "cloud": [
          "dropbox",
          "owncloud",
          "ubuntuone"
        ],
        "desktops": [
          "cinnamon"
        ],
        "devel": [
          "git",
          "juju",
          "zsh"
        ],
        "email": [
          "geary",
          "thunderbird"
        ],
        "eyecandy": [
          "faenza-icons",
          "faience-icons",
          "moka-icons",
          "nitrux-icons",
          "unsettings"
        ],
        "fileshare": [
          "jdownloader",
          "transmission"
        ],
        "graphics": [
          "pencil",
          "pinta",
          "shotwell"
        ],
        "graphics_pro": [
          "darktable",
          "draftsight",
          "freecad",
          "gimp",
          "inkscape",
          "mypaint",
          "qcad",
          "rawtherapee",
          "scribus"
        ],
        "groupware": [
