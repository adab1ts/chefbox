{
  "name": "@@NODE_NAME@@",
  "description": "Profile for @@NODE_NAME@@ node",
  "json_class": "Chef::Role",
  "chef_type": "role",
  "env_run_lists": {},
  "run_list": [
    "base::run-init",@@RUN_LIST@@,"base::run-end"
  ],
  "default_attributes": {
    "chef_report": {
      "recipient": "YOU@EXAMPLE.COM"
    },
    "box": {
      "name": "HOSTNAME",
      "platform": {
        "os": "OS",
        "version": "VERSION",
        "arch": "ARCH",
        "desktop": "DESKTOP",
        "virtual": false
      },
      "greeter": {
        "author": "AUTHOR",
        "background": "BACKGROUND",
        "resolution": "RESOLUTION"
      },
      "lang": "LANG",
      "folders": {
        "admin": "ADMIN_FOLDER",
        "apps": "APPS_FOLDER",
        "desktop": "DESKTOP_FOLDER"
      },
      "users": {
        "USER": {
          "uid": 1000,
          "gid": 1000,
          "group": "GROUP",
          "home": "/home/USER",
          "shell": "/bin/bash",
          "password": "secret",
          "default": true,
          "guest": false,
          "sudo": true,
          "system": false
        }
      },
      "support": {
        "users": ["USER"],
        "folder": "FIRSTSTEPS_FOLDER"
      },
      "dotfiles": {
        "users": ["USER"],
        "folder": ".dotfiles"
      },
      "jobs": {
        "users": ["USER"],
        "folder": "ADMIN_FOLDER/jobs",
        "profiles": {
          "USER": {
            "backup": {
              "tasks": [
                {
                  "name": "backup-DRIVE",
                  "drive": "DRIVE_FOLDER",
                  "folder": "BACKUP_FOLDER",
                  "targets": ["home"]
                }
              ]
            }
          }
        }
      },
      "devel": {
        "users": ["USER"],
        "folder": "DEVEL_FOLDER",
        "bin_folder": "BIN_FOLDER"
      },
      "apps": {
        "audio": [
          "nuvola",
          "rhythmbox",
          "spotify",
          "tomahawk"
        ],
        "audio_pro": [
          "ardour",
          "audacity",
          "guitarix",
          "hydrogen",
          "idjc",
          "mixxx",
          "muse",
          "musescore",
          "rosegarden",
          "transcribe",
          "traverso",
          "tuxguitar"
        ],
        "browsers": [
          "chrome",
          "chromium",
          "firefox",
          "midori",
          "opera"
        ],
        "cloud": [
          "drive",
          "dropbox",
          "owncloud"
        ],
        "devel": [
          "ansible",
          "atom",
          "bower",
          "brackets",
          "chefdk",
          "firefox-de"
          "git",
          "grunt",
          "gulp",
          "juju",
          "nodejs",
          "processing",
          "ruby",
          "shelr",
          "vagrant",
          "virtualbox",
          "webdesigner",
          "wp-devel",
          "wsk",
          "yeoman",
          "zsh"
        ],
        "email": [
          "geary",
          "thunderbird"
        ],
        "eyecandy": [
          "faba-icons",
          "moka-icons",
          "moka-theme",
          "numix-circle-icons",
          "numix-shine-icons",
          "orchis-theme",
          "unity-tweak"
        ],
        "fileshare": [
          "btsync",
          "filezilla",
          "frostwire",
          "jdownloader",
          "transmission",
          "uget"
        ],
        "graphics": [
          "pencil",
          "pinta",
          "shotwell"
        ],
        "graphics_pro": [
          "darktable",
          "draftsight",
          "freecad",
          "gimp",
          "inkscape",
          "mypaint",
          "qcad",
          "rawtherapee",
          "scribus"
        ],
        "groupware": [
          "gobby",
          "hangouts",
          "hexchat",
          "mumble",
          "smuxi",
          "skype"
        ],
        "indicators": [
          "plug&play",
