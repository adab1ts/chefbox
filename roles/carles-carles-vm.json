  "default_attributes": {
    "box": {
      "id": "carles-carles-vm",
      "name": "carles-vm",
      "default_user": "carles",
      "default_group": "carles"
    }
  },
  "override_attributes": {
    "jobs": {
      "job_list": {
        "backup": {
          "tasks": [
            {
              "name": "backup-my_passport",
              "drive": "My Passport",
              "folder": "MyData",
              "targets": ["home"]
            }
          ]
        }
      }
    }
  },
  "run_list": [
    "recipe[base]",
    "recipe[jobs]"
  ],
  "env_run_lists": {}
}
