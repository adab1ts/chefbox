# File: $ADMIN_PATH/dotfiles/bash/aliases.d/apt-get.bal


alias ag='sudo apt-get'
alias agc='sudo apt-get autoclean'
alias agc!='sudo apt-get clean'
alias agr='sudo apt-get autoremove'
alias agup='sudo apt-get update'
alias agug='sudo apt-get upgrade'
alias agi='sudo apt-get install'   # agi packagename
alias agp='sudo apt-get purge'     # agp packagename

