# vim:ft=zsh
# File: $DOTFILES_PATH/zsh/aliases.d/juju.zal


## juju aliases

alias jjb='juju bootstrap'
alias jjd='juju deploy'           # jjd charm name
alias jjau='juju add-unit'        # jjau charm_name [-nN]
alias jjar='juju add-relation'    # jjar charm_name charm_name
alias jje='juju expose'           # jje charm_name
alias jjs='juju status'           # jjs [charm_name]
alias jjdl='juju debug-log'

